{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Boiler plate for React client and server project",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terensu-desu/react-boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Terence Mangram",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/terensu-desu/react-boilerplate/issues"
  },
  "homepage": "https://github.com/terensu-desu/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.1.4"
  }
}
